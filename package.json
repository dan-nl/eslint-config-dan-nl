{
  "name": "eslint-config-dan-nl",
  "version": "0.1.5",
  "description": "an ESLint shareable config",
  "author": "dan entous",
  "license": "MIT",
  "main": "src",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dan-nl/eslint-config-dan-nl"
  },
  "keywords": [
    "eslint",
    "eslintconfig"
  ],
  "bugs": {
    "url": "https://github.com/dan-nl/eslint-config-dan-nl/issues"
  },
  "homepage": "https://github.com/dan-nl/eslint-config-dan-nl#readme",
  "scripts": {
    "cover:browser": "npm test -- --coverage-report=lcov",
    "cover:cli": "npm test -- --cov",
    "cover:travis": "npm test -- --coverage-report=lcov --no-browser",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "eslint": "eslint .",
    "pre-commit": "echo 'pre-commit checks ...' && npm run eslint && npm test -- --cov",
    "test": "tap test/**/*.test.js --reporter spec"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run pre-commit"
    }
  },
  "devDependencies": {
    "eslint": "^4.19.1",
    "ghooks": "^2.0.4",
    "tap": "^12.0.1"
  }
}
